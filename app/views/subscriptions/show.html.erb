<header>

<section>
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading"> <%= raw current_user.program.title %> </h2>
                   
                </div>
            </div>
            <div class="row">
               
                <div class="col-md-6 col-md-offset-3 user-program video-container">
                    
                     
             <%= raw current_user.program.code_one %> 
                                            
                </div>
            </div>
        <div>
          
           <button class="btn btn-success btn-round btn-fill"> Next Workout</button> 

    <button class="btn btn-success btn-round btn-fill program-two" data-toggle="modal" data-target="#myModal">Launch demo modal</button>    
     
    <button class="btn btn-success btn-round btn-fill program-three" data-toggle="modal" data-target="#myModal">
    
      Launch demo modal
    
    </button> 
        </div>
             

        <script type="text/javascript">          
        jQuery(document).ready(function(){
        $( ".program-one" ).click(function() { 
         $('#program-one').modal('show');
       });

        $( ".program-two" ).click(function() { 
         $('#program-two').modal('show');
       });

        $( ".program-three" ).click(function() { 
         $('#program-three').modal('show');

       });
        });//end script
      </script>
       


    </header>

   
    
    <!-- Modal -->
    <div class="row" >
    <div class="modal fade col-md-6 col-md-offset-3" id="program-two" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    
      
    
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    
            <h4 class="modal-title" id="myModalLabel">Modal title</h4>
    
        
    
          <div class="modal-body">
              
              <%= raw current_user.program.code_two %>
          </div>
  
    
    </div>
    </div>
    
    
    <!-- Modal -->
    
    <div class="modal fade" id="program-three" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    
      <div class="modal-dialog">
    
        <div class="modal-content">
    
          <div class="modal-header">
    
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    
            <h4 class="modal-title" id="myModalLabel">Modal title</h4>
    
          </div>
    
          <div class="modal-body">
              
              <%= raw current_user.program.code_three %>
          </div>
    
  
    
        </div>
    
      </div>
    
    </div>
    

</section>
</div>
