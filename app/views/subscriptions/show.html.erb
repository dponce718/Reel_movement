<header>

<section>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading"> <%= raw current_user.program.title %> </h2>
                   
                </div>
            </div>
            <div class="row">
                <div class="col-md-2">
            
                  <i class="fa fa-chevron-left fa-5x"></i> </i>      
                  
                </div>
                <div class="col-md-8 user-program">
                    
                     
             <%= raw current_user.program.code_one %> 
             
              
              </ul>

              <button class="btn btn-success btn-round btn-fill .program-one"> Next Workout</button>     
                                    
                </div>
                <div class="col-md-2">
                   <i class="fa fa-chevron-right fa-5x"></i>
                </div>
            </div>
        </div>
             

        <script type="text/javascript">          
        jQuery(document).ready(function(){
        $( ".program-one" ).click(function() { 
         $('#program-one').modal('show');
       });

        $( ".program-two" ).click(function() { 
         $('#program-two').modal('show');
       });

        $( ".program-three" ).click(function() { 
         $('#program-three').modal('show');
       });
        });//end script
      </script>
       


    </header>

  </section>  

  
    
    
    <!-- Modal -->
    
    
    <button class="btn btn-primary program-two" data-toggle="modal" data-target="#myModal">
    
      Launch demo modal
    
    </button>
    
    
    
    <!-- Modal -->
    
    <div class="modal fade" id="program-two" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    
      <div class="modal-dialog">
    
        <div class="modal-content">
    
          <div class="modal-header">
    
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    
            <h4 class="modal-title" id="myModalLabel">Modal title</h4>
    
          </div>
    
          <div class="modal-body">
              
              <%= raw current_user.program.code_two %>
          </div>
    
  
    
        </div>
    
      </div>
    
    </div>


    <button class="btn btn-primary program-three" data-toggle="modal" data-target="#myModal">
    
      Launch demo modal
    
    </button>
    
    
    
    <!-- Modal -->
    
    <div class="modal fade" id="program-three" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    
      <div class="modal-dialog">
    
        <div class="modal-content">
    
          <div class="modal-header">
    
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    
            <h4 class="modal-title" id="myModalLabel">Modal title</h4>
    
          </div>
    
          <div class="modal-body">
              
              <%= raw current_user.program.code_three %>
          </div>
    
  
    
        </div>
    
      </div>
    
    </div>
    


